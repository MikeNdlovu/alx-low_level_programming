#include "main.h"
#include <stdio.h>
#include <string.h>
#include <stdlib.h>

/**
 * _strstr - locates a substring
 * @needle: the substring being located
 * @haystack: the string from which the substring is being located
 * Return: pointer containing information
 */
char *_strstr(char *haystack, char *needle)
{
	char *locate;

	locate = strstr(haystack, needle);

	return (locate);
}
