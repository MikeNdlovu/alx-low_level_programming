#include "main.h"
#include <stdio.h>

/**
 * _isupper - checks for uppercase letters
 * @c: letter being checked
 * Return: 1 for uppercase & 0 otherwise
 */
int _isupper(int c)
{
	if (c <= 'A' && c <= 'Z')
	{
	_putchar(c + '0');
	_putchar(':');
	_putchar(' ');
	c++;

	return (1);
	_putchar('\n');
	}
	else 
	{
	return (0);
	_putchar('\n');
	}
}
	
